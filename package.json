{
  "name": "mun-sdk-ai-v2",
  "version": "3.21.0",
  "description": "MUN SDK AI V2 - AI Android Controller with Enhanced Features",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "version:patch": "node scripts/bump-version.js patch",
    "version:minor": "node scripts/bump-version.js minor",
    "version:major": "node scripts/bump-version.js major",
    "release": "node scripts/bump-version.js minor && npm run tauri:build",
    "release:patch": "node scripts/bump-version.js patch && npm run tauri:build",
    "release:upload": "node scripts/upload-release.js",
    "release:full": "node scripts/bump-version.js minor && npm run tauri:build && node scripts/upload-release.js",
    "test:e2e": "wdio run wdio.conf.js",
    "test:driver": "tauri-driver --native-driver ./msedgedriver.exe",
    "test:tauri": "concurrently --kill-others --success first \"npm run test:driver\" \"wait-on tcp:4444 && npm run test:e2e\""
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@wdio/cli": "^9.22.0",
    "@wdio/local-runner": "^9.22.0",
    "@wdio/mocha-framework": "^9.22.0",
    "@wdio/spec-reporter": "^9.20.0",
    "concurrently": "^9.2.1",
    "vite": "^5.0.0",
    "wait-on": "^9.0.3"
  }
}
